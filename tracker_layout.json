    // Pinned Item Grid
    "tracker_capture_item": {
        "type": "container",
        "content": {
          "type": "array",
          "orientation": "horizontal",
			    "content": [ 
            // Seeds
				    {
              "type": "itemgrid",
              "h_alignment": "center",
              "item_margin": "1,2",
              "rows": [
                ["emberseeds"],
                ["scentseeds"],
                ["pegasusseeds"],
                ["galeseeds"],
                ["mysteryseeds"]
              ]
            },
            // Equipment/Key Items
            {
              "type": "itemgrid",
              "h_alignment": "center",
              "item_margin": "1,2,5,10",
              "margin": "10,6,0,0",
              "rows": [
                ["sword1", "shield", "feather", "boomerang", "slingshot1", "season", "satchel"],
                ["bracelet", "magnet", "shovel", "flute_generic", "foolsore", "hook1", "cane"],
