                ["banana","ribbon", "gnarledkey", "floodkey", "dragonkey", "rustybell", "jewels"]
              ]
            }
          ]
        }
    },
    // Tracker Layout
    "tracker_default": {
      "type": "container",
      "background": "#595959",
      "content": {
        "type": "dock",
        "dropshadow": true,
        "content": [
          // Left-side of Tracker
          {
            "type": "dock",
            "dock": "left",
            "margin": "0,0,3,0",
            "content": [
              // Key Items/Essences
              {
                "type": "group",
                "header": "Key Items/Essences",
                "dock": "top",
                "content": {
                  "type": "layout",
                  "margin": 0,
                  "key": "shared_item_grid"
                }
              },
              // Seasons & Natzu
              {
                "type": "group",
                "header": "Seasons",
                "max_height": 205,
                "dock":"top",
                "content": {
                  "type": "itemgrid",
                  "item_size": 64,
                  "margin": "0, 5, 0, 0",
                  "item_margin": "7,-5,0,-20",
                  "rows": [
                    ["north_label", "north_winter", "swamp_label", "swamp_fall"],
                    ["suburbs_label", "suburbs_fall", "sunken_label", "sunken_summer"],
                    ["wow_label", "wow_summer", "coast_label", "season_coast"],
                    ["plain_label", "plain_spring", "temple_label", "temple_winter"]		  
                  ]
                }
              },
              // Portals
              {
                "type": "group",
                "header": "Portals",
                "content": {
                  "type": "itemgrid",
                  "item_size": 40,
                  "margin": "0, 5, 0, 0",
                  "item_margin": "0,0,3,5",
                  "rows": [
                    ["portal_1","portal_dance","","portal_2","portal_market"],
                    ["portal_3","portal_hns","","portal_4","portal_furnace"],
                    ["portal_5","portal_pirate","","portal_6","portal_volcano"],
                    ["portal_7","portal_sns"]
                  ]
                }
              }
            ]
          },
          // Right-side of Tracker
          {
            "type": "dock",
            "v_alignment": "stretch",
            "h_alignment": "stretch",
            "margin": "0,0,0,0",
            "content": [
              // Maps
              {
                "type": "dock",
                "dock": "top",
                "max_height": 600,
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "content": [
                  {
                    "type": "map",
                    "maps": [
                      "holodrum","subrosia"
                    ]
                  }
                ]
              },
              // Pinned Locations
              {
                "type": "group",
                "header": "Pinned Locations",
                "content": {
                  "type": "recentpins",
                  "style": "wrap",
                  "h_alignment": "stretch",
                  "v_alignment": "stretch",
                  "orientation": "horizontal",
                  "compact": true
                }
              }
            ]
